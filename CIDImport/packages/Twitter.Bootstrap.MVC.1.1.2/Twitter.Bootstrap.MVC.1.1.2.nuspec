<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Twitter.Bootstrap.MVC</id>
    <version>1.1.2</version>
    <title>Bootstrap for MVC 4</title>
